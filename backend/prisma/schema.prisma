// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Organization {
  id          String @id @default(uuid())
  name        String
  description String

  @@map("organization")
}

model FundingRound {
  id             String   @id @default(uuid())
  name           String
  amount         Int
  createdAt      DateTime
  organizationId String

  @@map("funding_round")
}